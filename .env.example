# RAD Gateway runtime
RAD_LISTEN_ADDR=:8090
RAD_RETRY_BUDGET=2

# API keys (name:secret,name:secret)
# IMPORTANT: set real values in .env and never commit .env
RAD_API_KEYS=default:replace-with-real-key

# Provider credentials (set as needed, examples only)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=

# Optional guardrails MCP endpoint for operator workflow
GUARDRAILS_MCP_SSE_URL=
GUARDRAILS_MCP_API_KEY=

# Infisical (secrets source of truth)
# Keep real values only in `.env` (git-ignored).
INFISICAL_API_URL=http://127.0.0.1:8080
INFISICAL_PROJECT_SLUG=
INFISICAL_SERVICE_TOKEN=

# Optional: workspace/project id (fallback when slug lookup is forbidden for service tokens)
INFISICAL_WORKSPACE_ID=
